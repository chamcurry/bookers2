<div class="container px-5">
  <div class="row">
   <%= render 'list', book: @book,user: @user %>
    <div class="col-md-8 offset-md-1">
     <h2>Books</h2>
     <table class="table">
      <thead>
        <tr>
          <th></th>
          <th>Title</th>
          <th>Opinion</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>
            <%=link_to user_path(params[:id]) do %>
             <%=image_tag @user.get_profile_image(100,100) %>
            <% end %>
          </td>
          <td><%=%></td>
          <td><%=%></td>
        </tr>
      </tbody>
     </table>
    </div>
  </div>
</div>
